#include "Car.h"

Car::Car() {

	// TODO Auto-generated constructor stub

}

Car::~Car() {
	// TODO Auto-generated destructor stub
}

uint16_t Car::getHeight() const {
	return height;
}

void Car::setHeight(uint16_t height) {
	this->height = height;
}

uint8_t Car::getObjSpeed() const {
	return obj_speed;
}

void Car::setObjSpeed(uint8_t objSpeed) {
	this->obj_speed = objSpeed;
}

uint16_t Car::getPosX() const {
	return pos_x;
}

void Car::setPosX(uint16_t posX) {
	this->pos_x = posX;
}

uint16_t Car::getPosY() const {
	return pos_y;
}

void Car::setPosY(uint16_t posY) {
	this->pos_y = posY;
}

uint16_t Car::getWidth() const {
	return width;
}

void Car::setWidth(uint16_t width) {
	if(width < 10) this->width = 10;
	else this->width = width;

}
